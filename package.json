{
  "name": "bootkempik",
  "version": "1.3.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build --configuration=production",
    "deploy": "npm run prebuild && ng build --configuration=production && firebase deploy --only hosting && npm run realize-new-version",
    "realize-new-version": "node ./realize-new-version.js",
    "analyze": "ng build --configuration=production  --statsJson=true && npx webpack-bundle-analyzer ./dist/stats.json",
    "deploy function": "firebase deploy --only functions",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "run-precashe": "npm run precache",
    "postinstall": "ngcc",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@angular/service-worker": "~11.0.2",
    "firebase": "8.1.2",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ngx-viewerjs": "^1.0.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "viewerjs": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "^11.2.8",
    "@angular/compiler-cli": "11.0.2",
    "@angular/service-worker": "^11.0.9",
    "@angular/language-service": "~11.0.2",
    "@firebase/app": "^0.6.13",
    "@firebase/database": "^0.8.1",
    "@firebase/messaging-types": "^0.5.0",
    "@types/chart.js": "^2.9.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.7",
    "codelyzer": "^6.0.0",
